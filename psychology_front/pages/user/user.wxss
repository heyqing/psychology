page {
  background-color: #F6F6F4;
}
.user_info .user_info_bg {
  position: relative;
  height: 40vh;
  background-color: #158bb8;
}
.user_info .user_info_bg .user_info_warp {
  position: relative;
  top: 30%;
  text-align: center;
}
.user_info .user_info_bg .user_info_warp .user_icon {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.user_info .user_info_bg .user_info_warp .user_name {
  color: #fff;
}
.back-img{
  background-image: linear-gradient(rgba(255, 255, 255, 0.8), rgba(255, 255, 255, 0.8)),url('https://meisheng-shop-img.oss-cn-beijing.aliyuncs.com/login.jpg');  
  background-position: center;  
  background-size: 100% 100%;  
  background-repeat: no-repeat;
}
.app_info_warp {
  padding: 20rpx;
}
.app_info_warp .app_info_item {
  display: flex;
  justify-content: left;
  padding: 20rpx;
  padding-left: 35rpx;
  border-bottom: 5rpx solid #efefef;
}
.app_info_warp .app_info_item image{
  height: 45rpx;
  width: 45rpx;
}
.app_info_warp .app_info_item text{
  padding-left: 15rpx;
}
.app_info_warp .feedback {
  margin: 0;
  padding: 0;
  background-color: transparent;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: start;
  align-items: center;
  font-size: 28rpx;
  font-weight: normal;
  flex-grow: 1;
}
.app_info_warp .feedback::after {
  border: none;
}
