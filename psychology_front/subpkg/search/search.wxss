page {
  background-color: #F9F9F9;
  padding-top: 20rpx;
}
.search_row {
  height: 60rpx;
  display: flex;
}
.search_row input {
  background-color: #FFFFFF;
  flex: 1;
  height: 100%;
  padding-left: 30rpx;
}
.search_row button {
  display: flex;
  width: 110rpx !important;
  height: 100%;
  font-size: 26rpx;
  padding: 0;
  margin: 0 10rpx;
  justify-content: center;
  align-items: center;
}
.search_content {
  margin-top: 15rpx;
  padding: 0px;
}
.search_content .search_item {
  background-color: #FFFFFF;
  font-size: 26rpx;
  padding: 15rpx;
  align-items: center;
  border-bottom: 2rpx solid #EAEAEA;
  display: flex;
}
.search_content .search_item .name{
  font-size: larger;
  font-weight: bolder;
}